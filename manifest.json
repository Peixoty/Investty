{
    "manifest_version": 3,
    "name": "Investty!",
    "description": "Adicione ativos da bolsa e acompanhe sua cotação. Fique ligado nas notícias de cada ativo também",
    "version": "1.1",
    "icons": {
    "16": "./icons/icon16x16.png",
    "128": "./icons/icon128x128.png"
  },
    "permissions": [
      "notifications",
      "storage",
      "activeTab",
      "alarms",
      "scripting"
    ],
    "host_permissions": [
      "https://news.google.com/rss/*"
    ],
    "background": {
      "service_worker": "./backend/background.js"
    },
    "action": {
      "default_popup": "./public/popup.html",
      "default_icon": {
      "16": "./icons/icon16x16.png",
      "128": "./icons/icon128x128.png"
    }
    }
  }
  